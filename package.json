    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.10",
    "@types/nodemailer": "^7.0.9",
    "@types/pg": "^8.16.0",
    "@types/serverless": "^3.12.27",
    "bcryptjs": "^3.0.3",
    "better-auth": "^1.4.17",
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "http-status": "^2.1.0",
    "nodemailer": "^7.0.12",
    "pg": "^8.17.2",
    "prisma": "^7.3.0",
    "serverless": "^4.31.2",
    "serverless-http": "^4.0.0",
    "slugify": "^1.6.6",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "vercel": "^50.9.1"
  }
